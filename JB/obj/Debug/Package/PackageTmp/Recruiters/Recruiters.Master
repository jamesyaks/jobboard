<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Recruiters.Master.cs"
    Inherits="JB.Recruiters.Rec" %>

<!DOCTYPE HTML>
<html>
<head runat="server">
    <title>Recruiters</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width" />
    <meta name="keywords" content="jobboard, jobboard software, jobboard code, jobsite, jobsite, ahrcloud, jobboard branding, jobboard source code, jobboard community edition" />
    <meta name="description" content="Job board software by ahrcloud. Free light version community edition" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <link rel="SHORTCUT ICON" href="/images/favicon.ico" />
    <link href="/styles/csrecruiter.css" rel="stylesheet" type="text/css" />
    <link href="/styles/cs2.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <form id="form1" class="ux_upperwrap" runat="server">
    <div id="cleftpanel">
        <ul class="ux_flist">
            <li>
                <asp:Image ID="Image11" CssClass="ux_rc_home" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton4" runat="server" CausesValidation="False" CssClass="ux_recmasterwhite"
                    OnClick="LinkButton44_Click" Width="100px">Site Home</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image13" CssClass="ux_rc_dashboard" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton9" runat="server" CausesValidation="False" CssClass="ux_recmasterwhite"
                    OnClick="LinkButton4_Click" Width="100px">Dashboard</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image1" CssClass="ux_rc_basic" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton5" runat="server" CssClass="ux_recmasterwhite" OnClick="LinkButton5_Click"
                    CausesValidation="False" Width="100px">Basic Information</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image12" CssClass="ux_rc_app" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton8" runat="server" CssClass="ux_recmasterwhite" OnClick="LinkButton8_Click"
                    Width="100px" CausesValidation="False">My Applications</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image5" CssClass="ux_rc_edit" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton6" runat="server" CausesValidation="False" CssClass="ux_recmasterwhite"
                    OnClick="LinkButton6_Click" Width="100px">Edit Jobs</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image10" CssClass="ux_rc_password" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton3" runat="server" CssClass="ux_recmasterwhite" OnClick="LinkButton3_Click"
                    CausesValidation="False" Width="100px">Change password</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image6" CssClass="ux_rc_post" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:LinkButton ID="LinkButton7" runat="server" CausesValidation="False" CssClass="ux_recmasterwhite"
                    OnClick="LinkButton7_Click" Width="100px">Post a job</asp:LinkButton>
            </li>
            <li>
                <asp:Image ID="Image4" CssClass="ux_rc_logout" runat="server" ImageUrl="/images/sprite.gif" />
                <asp:HyperLink ID="HyperLink5" runat="server" CssClass="ux_recmasterwhite" NavigateUrl="~/Logoff.aspx"
                    Width="100px">Logout</asp:HyperLink>
            </li>
        </ul>
    </div>
    <div id="crightpanel">
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
        <footer>
            <!-- bottom maintain compatibility without html5-->
            <div class="ux_cfooter">
                <asp:Label ID="Label1" runat="server" CssClass="ux_ftblack"></asp:Label>
            </div>
        </footer>
    </div>
    <div class="ux_cleardata">
    </div>
    </form>
</body>
</html>